{
  "name": "florian-escribano-website",
  "version": "1.0.0",
  "description": "Personal website for Florian Escribano - Built with Eleventy",
  "scripts": {
    "build:css": "sass _sass/main.scss:assets/css/main.css _sass/ie8.scss:assets/css/ie8.css _sass/ie9.scss:assets/css/ie9.css _sass/noscript.scss:assets/css/noscript.css",
    "watch:css": "sass --watch _sass/main.scss:assets/css/main.css _sass/ie8.scss:assets/css/ie8.css _sass/ie9.scss:assets/css/ie9.css _sass/noscript.scss:assets/css/noscript.css",
    "build:eleventy": "eleventy",
    "watch:eleventy": "eleventy --serve",
    "build": "npm run build:css && npm run build:eleventy",
    "start": "npm-run-all build:css --parallel watch:*",
    "serve": "npm start"
  },
  "keywords": [
    "eleventy",
    "static-site",
    "portfolio"
  ],
  "author": "Florian Escribano",
  "license": "CCA-3.0",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7"
  }
}
